pipeline {
    agent any
    stages {
        stage('Cleaning Phase') {
            steps {
                bat "mvn clean"
            }
        }
        stage('Testing Phase') {
            steps {
                bat "mvn test"
            }
        }
        stage('Packaging Phase') {
            steps {
                bat "mvn package"
            }
        }

        stage('Consolidate Results'){
             steps {
                junit '**/target/surefire-reports/Test-*.xml'
                 archive 'target/*.jar'
            }
           
        }
    }
}